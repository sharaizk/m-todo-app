{
  "expo": {
    "name": "MyTaskList",
    "slug": "m-todo-app",
    "version": "1.1.0(Beta)",
    "orientation": "portrait",
    "icon": "./assets/applogo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/applogo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.sharaiz_k.myTaskList"
    },
    "web": {
      "favicon": "./assets/applogo.png"
    }
  }
}
